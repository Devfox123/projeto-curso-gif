algoritmo escola;
// Síntese
//  Objetivo:  Parabenizar o estudante com nota 10
//  Entrada :  	Nota	
//  Saída   :	Mensagem de parabéns


principal
	// Declarações
	real nota;
	// Instruções
	escreva("Digite uma nota: ");
	leia(nota);
	se (nota == 10) entao
		escreva("Parabéns!");
	fimSe
fimPrincipal
